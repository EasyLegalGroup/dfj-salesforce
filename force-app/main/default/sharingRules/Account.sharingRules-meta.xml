<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>DK_Host_Account_Sharing</fullName>
        <accessLevel>Edit</accessLevel>
        <accountSettings>
            <caseAccessLevel>Edit</caseAccessLevel>
            <contactAccessLevel>Edit</contactAccessLevel>
            <opportunityAccessLevel>Read</opportunityAccessLevel>
        </accountSettings>
        <description>Shares all Host Accounts with Event Coordinators</description>
        <label>DK: Host Account Sharing</label>
        <sharedTo>
            <group>Event_Coordination_Denmark</group>
        </sharedTo>
        <criteriaItems>
            <field>Host_Type__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <criteriaItems>
            <field>Market_Unit__c</field>
            <operation>equals</operation>
            <value>Din Familie Jurist,Testamente Danmark</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
