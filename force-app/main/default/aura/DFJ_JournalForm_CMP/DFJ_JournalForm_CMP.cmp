<aura:component description="DFJ_JournalForm" implements="force:lightningQuickActionWithoutHeader,force:hasRecordId,force:hasSObjectName,flexipage:availableForAllPageTypes" controller="DFJ_JournalForm" access="global">

    <ltng:require styles="{!$Resource.DFj_JournalCSS}" />

    <!-- <aura:attribute name="activeSections" type="List" default="['PreQuestioning']" /> -->
    <aura:attribute name="sObjectName" type="String" />
    <aura:attribute name="activeSectionsMessage" type="String" default="" />
    <aura:attribute name="JournalData" type="Journal__c" access="global" default="{'sobjectType':'Journal__c'}" />
    <aura:attribute name="spouse1Data" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="spouse2Data" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="spouse1childInstanceData" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="spouse2childInstanceData" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="childDataCommonInstance" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="childdataprocuratorInstance" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="assetMoneyInstance" type="Assests__c" access="global" default="{'sobjectType':'Assests__c'}" />
    <aura:attribute name="assetThingInstance" type="Assests__c" access="global" default="{'sobjectType':'Assests__c'}" />
    <aura:attribute name="guardianInstance" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="unwantedGuardianInstance" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="heirpersonInstance" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="heir2personInstance" type="Person__c" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="updateLeadFields" type="Lead" access="global" default="{'sobjectType':'Lead'}" />
    <aura:attribute name="childInstanceEditIndex" type="Integer" access="global" />
    <aura:attribute name="inheritanceEditIndex" type="Integer" access="global" />
    <aura:attribute name="commonChildInstanceEditIndex" type="Integer" access="global" default='' />
    <aura:attribute name="procuratorChildDataEditIndex" type="Integer" access="global" default="" />
    <aura:attribute name="assetMoneyInstanceEditIndex" type="Integer" access="global" />
    <aura:attribute name="assetThingInstanceEditIndex" type="Integer" access="global" />
    <aura:attribute name="guardianInstanceEditIndex" type="Integer" access="global" />
    <aura:attribute name="assetMoney" type="Assests__c[]" access="global" default="{'sobjectType':'Assests__c'}" />
    <aura:attribute name="childDatasp1" type="Person__c[]" access="global" default="[]" />
    <aura:attribute name="hierPersonData" type="Person__c[]" access="global" default="[]" />
    <aura:attribute name="hier2PersonData" type="Person__c[]" access="global" default="[]" />
    <aura:attribute name="childDatasp2" type="Person__c[]" access="global" default="[]" />
    <aura:attribute name="childDataProcurator" type="Person__c[]" access="global" default="[]" />
    <aura:attribute name="childDataCommon" type="Person__c[]" access="global" default="[]" />
    <aura:attribute name="assetThing" type="Assests__c[]" access="global" default="{'sobjectType':'Assests__c'}" />
    <aura:attribute name="guardianData" type="Person__c[]" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="unwantedGuardianData" type="Person__c[]" access="global" default="{'sobjectType':'Person__c'}" />
    <aura:attribute name="configurationData" type="Object[]" access="global" />
    <aura:attribute name="allPicklistValue" type="Object" default="{}" access="global" />
    <aura:attribute name="counterValue" type="Object" default="{}" access="global" />
    <aura:attribute name="errorLog" type="Log__c" access="global" default="{'sObjectType' : 'Log__c'}" />

    <aura:attribute name="ShowComponentButton" type="Boolean" default="false" access="global" />
    <aura:attribute name="disablePicklist" type="Boolean" default="false" access="global" />
    <aura:attribute name="disableButton" type="Boolean" default="false" access="global" />
    <aura:attribute name="callMaskingHelper" type="Boolean" default="false" access="global" />
    <aura:attribute name="counter" type="Integer" default="0" access="global" />
    <aura:attribute name="closeModal" type="Boolean" default="true" access="global" />
    <aura:attribute name="ShowComponentButton1" type="String" access="global" />
    <aura:attribute name="IsAssetMoneyEditMode" type="Boolean" default="false" access="global" />
    <aura:attribute name="disableSaveChildrenButton" type="Boolean" default="false" access="global" />
    <aura:attribute name="IsHeirAddEdit" type="Boolean" default="false" access="global" />
    <aura:attribute name="IsHeir2AddEdit" type="Boolean" default="false" access="global" />
    <aura:attribute name="IsAssetThingEditMode" type="Boolean" default="false" access="global" />
    <aura:attribute name="IsGuardianEditMode" type="Boolean" default="false" access="global" />
    <aura:attribute name="IsUnwantedGuardianEditMode" type="Boolean" default="false" access="global" />
    <aura:attribute name="hasValue" type="Boolean" default="false" access="global" />
    <aura:attribute name="PersonDeleteList" type="List" default="[]" access="global" />
    <aura:attribute name="childDataProcuratorDeleteList" type="List" default="[]" access="global" />
    <aura:attribute name="AssetDeleteList" type="List" default="[]" access="global" />
    <aura:attribute name="GuardianDeleteList" type="List" default="[]" access="global" />
    <aura:attribute name="heirDeleteList" type="List" default="[]" access="global" />
    <aura:attribute name="allChildren" type="List" default="[]" access="global" />
    <aura:attribute name="selectedValue" type="String" access="global" />
    <aura:attribute name="selectedChild" type="String" access="global" />
    <aura:attribute name="selectedValueforChildType" type="String" access="global" />
    <aura:attribute name="selectedValueForChildSP1" type="String" access="global" />
    <aura:attribute name="selectedValueForChildSP2" type="String" access="global" />
    <aura:attribute name="selectedValueForChildSPCommon" type="String" access="global" />
    <aura:attribute name="hasChildren" type="Boolean" default="false" access="global" />
    <aura:attribute name="IsInheritanceEditModal" type="Boolean" default="false" access="global" />
    <aura:attribute name="IsFutureProcurationModal" type="Boolean" default="false" access="global" />
    <aura:attribute name="showErrorpopup" type="Boolean" default="false" access="global" />
    <aura:attribute name="IsFieldDisabled" type="Boolean" default="false" access="global" />
    <aura:attribute name="showHideSections" type="Object" access="global" />
    <aura:attribute name="showHideFieldsUsingUniqueName" type="Object" access="global" />
    <aura:attribute name="childInstanceEditName" type="String" access="global" />
    <aura:attribute name="journalId" type="String" access="global" />
    <aura:attribute name="leadId" type="String" access="global" />
    <aura:attribute name="EventObj" type="Event" default="{'sobjectType':'Event'}" />
    <aura:attribute name="fillField" type="String" default="Udfyld venligst dette felt." access="global" />
    <aura:attribute name="isSpinnerOpen" type="Boolean" default="false" />
    <aura:attribute name="isModalOpen" type="Boolean" default="false" />
    <aura:attribute name="isConfigurationEmpty" type="Boolean" default="false" />
    <aura:attribute name="userList" type="User[]" />
    <aura:attribute name="EventTypePicklistValue" type="List" />
    <aura:attribute name="optionsForGuardianType" type="List" />
    <aura:attribute name="isTheLeadAndPersonValuesDifferent" type="Boolean" default="false" />
    <aura:attribute name="civilstatusOption" type="List" default="[
    {'label': 'Single', 'value': 'Single'},
    {'label': 'Gift', 'value': 'Married'},
    {'label': 'Samlevende', 'value': 'Cohabiting'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />
    <aura:attribute name="childrenTypeOption" type="List" default="[
    {'label': 'Fælles', 'value': 'Common'},
    {'label': 'Sammenbragte', 'value': 'Blended'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />
    <aura:attribute name="testamentTypeOption" type="List" default="[
    {'label': 'Vidne', 'value': 'Witness'},
    {'label': 'Notar', 'value': 'Registrar'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />

    <aura:attribute name="inheritanceFromLongestLivedOptions" type="List" default="[
    {'label': 'Ligedeling', 'value': 'Equal Sharing'},
    {'label': 'Arv efter gren', 'value': 'Inheritance by branch'},
    {'label': 'Procent fordeling', 'value': 'Percent distribution'},
    {'label': '30% løsning', 'value': '30% solution'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />

    <aura:attribute name="procuratorOrder" type="List" default="[
    {'label': 'prioriteret rækkefølge', 'value': 'prioritized order'},
    {'label': 'i forening', 'value': 'group'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />

    <aura:attribute name="guardianTypeOptions" type="List" default="[
    {'label': 'Primær', 'value': 'Primær'},
    {'label': 'Sekundær', 'value': 'Sekundær'},
    {'label': 'N/A', 'value': 'Missing'}

    ]" />
    <aura:attribute name="testationsretOption" type="List" default="[
    {'label': 'Ja', 'value': 'Ja'},
    {'label': 'Nej', 'value': 'Nej'},
    {'label': 'Kun 50%', 'value': 'Kun 50%'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />
    <aura:attribute name="inheritanceFirstOption" type="List" default="[
    {'label': 'Fuld begunstigelse partner', 'value': 'Fuld begunstigelse partner'},
    {'label': 'Fuld begunstigelse børn', 'value': 'Fuld begunstigelse børn'},
    {'label': 'Procentdeling', 'value': 'Procentdeling'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />
    <aura:attribute name="radioButtonsYesNoMissingOptions" type="List" default="[
    {'label': 'Ja', 'value': 'Yes'},
    {'label': 'Nej', 'value': 'No'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />

    <aura:attribute name="allowGuardianAmount" type="List" default="[
    {'label': 'Månedligt pr. Barn', 'value': 'Toggle_Guardian_monthly_amount__c'},
    {'label': 'Engangsbeløb til værgen', 'value': 'Toggle_Guardian_one_time_fee__c'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />
    <aura:attribute name="custodyTypeOptions" type="List" default="[
    {'label': 'Fælles', 'value': 'Common'},
    {'label': 'Delt', 'value': 'Divided'},
    {'label': 'Fuld', 'value': 'full'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />

    <aura:attribute name="separatePropertyTypeOptions" type="List" default="[
    {'label': 'Kombinationssæreje for arv m.v. til livsarvinger', 'value': 'Kombinationssæreje for arv m.v. til livsarvinger'},
    {'label': 'Fuldstændigt særeje for arv m.v. til livsarvinger', 'value': 'Fuldstændigt særeje for arv m.v. til livsarvinger'},
    {'label': 'Fuldstændigt særeje med succession', 'value': 'Fuldstændigt særeje med succession'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />

    <aura:attribute name="marriageTypeOption" type="List" default="[
    {'label': 'Almindeligt Formuefællesskab', 'value': 'Almindeligt formuefællesskab'},
    {'label': 'Ægtepagt / Særeje', 'value': 'Ægtepagt / Særeje'},
    {'label': 'N/A', 'value': 'Missing'}
    ]" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <!--Attributes to remove-->

    <aura:attribute name="IsEditMode" type="Boolean" default="false" access="global" />

    <!--Attributes changed-->
    <aura:attribute name="isChildModalOpen" type="Boolean" default="false" access="global" />
    <!--Changes DIN-383-->
    <!--Start-->
    <aura:attribute name="JournalRecordData" type="Journal__c" default="{'sobjectType':'Journal__c'}"/>
    <aura:attribute name="isCreateJournalHistoryOnLead" type="Boolean" default="false" access="global" description="to enable or disable the creation Of Journal History Record on Lead." />
    <aura:attribute name="isSetLastEditValueOnLead" type="Boolean" default="false" access="global" description="to enable or disable the creation Of Journal History Record Old Value when user click on Open Journal form button."/>
    <!--End-->
    <!-- Changes for ticket - DIN-169 : Open record content in form. -->
    <!-- Start -->
    <aura:attribute name="docFabFormUrl" type="String" description="contains docFab form url" />
    <aura:attribute name="openDocFabForm" type="Boolean" description="to open docFab form" />
    <aura:attribute name="isOpenJournalFormClicked" type="Boolean" default="false" />
    <!-- End -->
    <aura:attribute name="disableProcessDocfabButton" type="Boolean" default="false" description="to enable or disable the Process docfab Document button." />
    <lightning:navigation aura:id="navigationService" />

    <div class="slds-box" style="background: white;">
        <div class="slds-p-around_x-small">
            <lightning:button label="Open Journal form" class="slds-button_stretch slds-button_brand alignment" iconName="utility:edit_form" iconPosition="left" type="button" onclick="{!c.showJournalForm}" />
        </div>
        <div class="slds-p-around_x-small">
            <lightning:select name="" label="Journal Status:" value="{!v.JournalData.Journal_States__c}" required="true">
                <option value="">choose one...</option>
                <aura:iteration items="{!v.allPicklistValue.JournalStatesPicklistArray}" var="inPick">
                    <option value="{!inPick.value}">{!inPick.label}</option>
                </aura:iteration>
            </lightning:select>
        </div>

        <!-- Responsible lawyer -->
        <div class="slds-p-around_x-small">
            <lightning:select name="" label="Responsible Lawyer" value="{!v.JournalData.Responsible_Lawyer__c}">
                <option value="">choose one...</option>
                <aura:iteration items="{!v.userList}" var="user">
                    <option value="{!user.Id}">{!user.Name}</option>
                </aura:iteration>
            </lightning:select>
        </div>
        
        <!-- Retskreds--><!-- DFJ-160-->
         <div class="slds-p-around_x-small">
            <lightning:select name="" label="Retskreds" value="{!v.JournalData.Retskreds__c}">
                <option value="">choose one...</option>
                <aura:iteration items="{!v.allPicklistValue.JournalRetskredsPicklistArray}" var="inPick">
                    <option value="{!inPick.value}">{!inPick.label}</option>
                </aura:iteration>
            </lightning:select>
        </div>
        

        <!-- Changes for DIN-284 : Create type picklist field on journal and show in component -->
        <!-- Start -->
        <!--Changes Starts DFJ-106-->
        <!--div class="slds-p-around_x-small">
            <lightning:select name="" label="Type" value="{!v.JournalData.Type__c}">
                <aura:iteration items="{!v.allPicklistValue.picklistWrappersJournalTypeValues}" var="inPick">
                    <option value="{!inPick.value}">{!inPick.label}</option>
                </aura:iteration>
            </lightning:select>
        </div-->
        <!-- End -->

        <lightning:input name="Deadline" type="datetime" label="Dato for deadline" value="{!v.JournalData.journal_Deadline__c}" aura:id="Journal" class="slds-p-around_x-small" />
        <div align="left" class="slds-p-around_x-small">
            <lightning:button variant="brand" label="Schedule a meeting with attorney" title="Schedule" onclick="{!c.openModal}" />
        </div>
        <div class="slds-p-around_x-small">
            <label>Journal was created:</label>
            <p>
                <lightning:formattedDateTime label="" value="{!v.JournalData.CreatedDate}" year="2-digit" month="short" day="2-digit" weekday="long" />
            </p>
        </div>

        <div class="slds-p-around_x-small">
            <label>Last time Journal was edited:</label>
            <p>
                <lightning:formattedDateTime label="" value="{!v.JournalData.LastModifiedDate}" year="2-digit" month="short" day="2-digit" weekday="long" />
            </p>
        </div>

        <div class="slds-p-around_x-small">
            <label>Last time document was generated:</label>
            <aura:if isTrue="{!empty(v.JournalData.Document_generated_on__c)}">
                <p>No document has been generated.</p>
                <aura:set attribute="else">
                    <p>
                        <lightning:formattedDateTime label="" value="{!v.JournalData.Document_generated_on__c}" year="2-digit" month="short" day="2-digit" weekday="long" />
                    </p>
                </aura:set>
            </aura:if>
        </div>
        <!-- Changes for ticket DIN-197 : Process document template feature -->
        <!-- Added button to open the Docfab url for document -->
        <!-- Start -->
        <div class="slds-p-around_x-small">
            <lightning:button variant="brand" label="Process Docfab Document" title="Process Docfab Document" onclick="{!c.processDocfabDocument}" disabled="{!v.disableProcessDocfabButton}" />
        </div>
        <!-- End -->
        <div class="slds-p-around_x-small">
            <label>Validation:</label>
            <p>There are {!v.JournalData.Total_Count__c} fields left empty</p>
        </div>

        <aura:if isTrue="{!v.JournalData.Template_outdated__c}">
            <div class="slds-p-around_x-small">
                <label>Warning:</label>
                <p>The journal has been edited since
                    <lightning:formattedDateTime label="" value="{!v.JournalData.LastModifiedDate}" year="2-digit" month="short" day="2-digit" weekday="long" /> last document generation.</p>
            </div>
        </aura:if>

        <lightning:layout>
            <lightning:layoutItem size="12" largeDeviceSize="12" mediumDeviceSize="12" smallDeviceSize="12">
                <div align="right" class="slds-p-around_x-small">
                    <lightning:button variant="brand" label="Update" title="Update" onclick="{!c.updateFieldsOutsideForm}" />
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem size="12" largeDeviceSize="12" smallDeviceSize="12" mediumDeviceSize="12">
                <div>
                    <aura:if isTrue="{!v.isModalOpen}">

                        <!-- Modal/Popup Box starts here-->
                        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                            <div class="slds-modal__container">
                                <!-- Modal/Popup Box Header Starts here-->
                                <header class="slds-modal__header">
                                    <lightning:buttonIcon iconName="utility:close" onclick="{! c.openModal }" alternativeText="close" variant="bare-inverse" class="slds-modal__close" />
                                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Create Event
                                    </h2>
                                </header>
                                <!--Modal/Popup Box Body Starts here-->
                                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                                    <div class="slds-p-top_small">
                                        <lightning:layout multipleRows="true">

                                            <lightning:layoutItem class="slds-p-around_xx-small" size="12" largeDeviceSize="6" flexibility="grow" mediumDeviceSize="9" smallDeviceSize="9">
                                                <lightning:input label="Start date time" type="datetime-local" class="width100" value="{!v.EventObj.StartDateTime}" aura:id="appointMent" required="true" />
                                            </lightning:layoutItem>

                                            <lightning:layoutItem class="slds-p-around_xx-small paddingTop" size="12" largeDeviceSize="6" flexibility="grow" mediumDeviceSize="3" smallDeviceSize="3">
                                                <lightning:select label="Duration" aura:id="durationValue" value="60">
                                                    <option value="10">10 Mins</option>
                                                    <option value="20">20 Mins</option>
                                                    <option value="30">30 Mins</option>
                                                    <option value="40">40 Mins</option>
                                                    <option value="50">50 Mins</option>
                                                    <option value="60">60 Mins</option>
                                                </lightning:select>
                                            </lightning:layoutItem>


                                            <lightning:layoutItem class="slds-p-around_xx-small" size="12" largeDeviceSize="6" flexibility="grow" mediumDeviceSize="9" smallDeviceSize="9">
                                                <lightning:input label="Subject" type="text" class="width100" value="{!v.EventObj.Subject}" aura:id="appointMent" required="true" />
                                            </lightning:layoutItem>

                                            <lightning:layoutItem class="slds-p-around_xx-small" size="12" largeDeviceSize="6" flexibility="grow" mediumDeviceSize="6" smallDeviceSize="6">
                                                <lightning:select label="Appointment owner" value="{!v.EventObj.OwnerId}" aura:id="appointMent" required="true">
                                                    <option value="">No owner assigned</option>
                                                    <aura:iteration items="{!v.userList}" var="user">
                                                        <option value="{!user.Id}">{!user.Name}</option>
                                                    </aura:iteration>
                                                </lightning:select>
                                            </lightning:layoutItem>


                                            <lightning:layoutItem class="slds-p-around_xx-small" size="6" largeDeviceSize="6" flexibility="grow" mediumDeviceSize="6" smallDeviceSize="6">
                                                <lightning:select label="Appointment type" value="{!v.EventObj.Type}" aura:id="appointMent" required="true">
                                                    <aura:iteration items="{!v.EventTypePicklistValue}" var="user">
                                                        <option value="{!user.value}">{!user.label}</option>
                                                    </aura:iteration>
                                                </lightning:select>
                                            </lightning:layoutItem>

                                            <!-- Changes for DIN-180 : Add checkbox to schedule meeting with attorney   -->
                                            <!-- Start -->
                                            <aura:if isTrue="{!v.sObjectName == 'Journal__c'}">
                                                <lightning:layoutItem class="slds-p-around_xx-small" size="6" largeDeviceSize="6" flexibility="grow" mediumDeviceSize="6" smallDeviceSize="6">
                                                    <div class="slds-p-top_large">
                                                        <lightning:input type="checkbox" label="SMS Flow" checked="{!v.JournalData.SMS_Flow__c}" />
                                                    </div>
                                                </lightning:layoutItem>
                                            </aura:if>
                                            <!-- End -->

                                            <lightning:layoutItem class="slds-p-around_xx-small" size="12" largeDeviceSize="6" flexibility="grow" mediumDeviceSize="6" smallDeviceSize="6">
                                            </lightning:layoutItem>

                                            <lightning:layoutItem class="slds-p-around_xx-small" size="12" largeDeviceSize="12" flexibility="grow" mediumDeviceSize="12" smallDeviceSize="12">
                                                <lightning:textarea label="Description" class="min-Height-105" value="{!v.EventObj.Description}" />
                                            </lightning:layoutItem>

                                        </lightning:layout>
                                    </div>
                                </div>
                                <!--Modal/Popup Box Footer Starts here-->
                                <footer class="slds-modal__footer">
                                    <lightning:button variant="neutral" label="Cancel" title="Cancel" onclick="{! c.openModal }" />
                                    <lightning:button variant="brand" label="Create" title="OK" onclick="{!c.createEventOnJournal}" />
                                </footer>
                            </div>
                        </section>
                        <div class="slds-backdrop slds-backdrop_open"></div>
                    </aura:if>
                </div>
            </lightning:layoutItem>
        </lightning:layout>


    </div>
    <aura:if isTrue="{!v.closeModal}">
        <div class="{!v.ShowComponentButton1}">
            <aura:if isTrue="{!v.ShowComponentButton}">

                <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                    <div class="slds-modal__container">

                        <!--###### MODAL BOX BODY Part Start######-->
                        <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                            <div>
                                <aura:if isTrue="{!v.isSpinnerOpen}">
                                    <div>
                                        <lightning:spinner variant="brand" size="large" alternativeText="Loading" />
                                    </div>
                                </aura:if>
                                <div>
                                    <aura:if isTrue="{!v.isConfigurationEmpty}">
                                        <aura:if isTrue="{!v.sObjectName == 'Lead'}">
                                            <p>Please select a product from Product Selector.</p>
                                        </aura:if>

                                        <aura:if isTrue="{!v.sObjectName == 'Journal__c'}">
                                            <p>No Order is present</p>
                                        </aura:if>
                                    </aura:if>
                                    <div class="slds-grid slds-grid_vertical-align-center slds-grid_align-center test2">
                                        <div class="slds-col slds-size_4-of-6 slds-medium-size_6-of-6 slds-large-size_4-of-6 test3">
                                            <lightning:accordion allowMultipleSectionsOpen="false" onsectiontoggle="{! c.handleSectionToggle }" activeSectionName="PreQuestioning">
                                                <lightning:layout class="slds-grid_vertical" multipleRows="true">
                                                    <lightning:layoutItem size="12" largeDeviceSize="12" mediumDeviceSize="12" smallDeviceSize="12">
                                                        <!--Pre Questioning-->
                                                        <aura:if isTrue="{!v.showHideFieldsUsingUniqueName.PreQuestioning.isVisible}">
