        <name>varInternalSender</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS(LOWER({!Get_Email_Message.FromAddress}), &quot;@dinfamiliejurist.dk&quot;)</expression>
    </formulas>
    <formulas>
        <name>varInternalSender_nu</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS(LOWER({!Get_Email_Message.FromAddress}), &quot;@@dinfamiljejurist.nu&quot;)</expression>
    </formulas>
