    <formulas>
        <name>DK_Weekday_EventStart</name>
        <dataType>String</dataType>
        <expression>CASE(
   MOD( DATEVALUE( {!$Record.Start__c} ) - DATE(1900, 1, 7), 7),
   0, &quot;søndag&quot;,
   1, &quot;mandag&quot;,
   2, &quot;tirsdag&quot;,
   3, &quot;onsdag&quot;,
   4, &quot;torsdag&quot;,
   5, &quot;fredag&quot;,
   6, &quot;lørdag&quot;,
   &quot;Error&quot;
)</expression>
    </formulas>
    <formulas>
        <name>SE_Weekday_EventStart</name>
