    <decisions>
        <name>DK_Is_it_overbooked</name>
        <label>DK: Is it overbooked?</label>
        <locationX>314</locationX>
        <locationY>876</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>DK_Event_is_Overbooked</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Booked_Seats__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Record.Capacity__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>DK_Assign_Overbooked_True</targetReference>
            </connector>
            <label>DK: Event is Overbooked</label>
        </rules>
        <rules>
            <name>DK_Not_Overbooked</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Booked_Seats__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Capacity__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>DK_Assign_Overbooked_False</targetReference>
            </connector>
            <label>DK: Not Overbooked</label>
        </rules>
    </decisions>
