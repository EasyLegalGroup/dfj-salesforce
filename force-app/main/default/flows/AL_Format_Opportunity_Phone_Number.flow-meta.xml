    <formulas>
        <name>PhoneFormatted</name>
        <dataType>String</dataType>
        <expression>IF( 
    BEGINS(SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;), &apos;+45&apos;), 
    SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;),
    IF(
        BEGINS(SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;), &apos;45&apos;), 
        &apos;+&apos; + SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;),
        IF(
            AND(
                NOT(BEGINS(SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;), &apos;+45&apos;)), 
                OR( 
                    ISPICKVAL({!Loop_Opps.Market_Unit__c}, &apos;DFJ_DK&apos;), 
                    ISPICKVAL({!Loop_Opps.Market_Unit__c}, &apos;Testamente_DK&apos;)
                )
            ), 
            &apos;+45&apos; + SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;),
            IF(
                AND(
                    NOT(BEGINS(SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;), &apos;+&apos;)), 
                    OR( 
                        ISPICKVAL({!Loop_Opps.Market_Unit__c}, &apos;DFJ_DK&apos;), 
                        ISPICKVAL({!Loop_Opps.Market_Unit__c}, &apos;Testamente_DK&apos;)
                    )
                ), 
                &apos;+&apos; + SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;),
                IF(
                    BEGINS(SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;), &apos;+46&apos;), 
                    SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;),
                    IF(
                        BEGINS(SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;), &apos;46&apos;), 
                        &apos;+&apos; + SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;),
                        IF(
                            AND(
                                NOT(BEGINS(SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;), &apos;+46&apos;)), 
                                ISPICKVAL({!Loop_Opps.Market_Unit__c}, &apos;FA_SE&apos;)
                            ), 
                            &apos;+46&apos; + SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;),
                            IF(
                                AND(
                                    NOT(BEGINS(SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;), &apos;+&apos;)), 
                                    ISPICKVAL({!Loop_Opps.Market_Unit__c}, &apos;FA_SE&apos;)
                                ), 
                                &apos;+&apos; + SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;),
                                SUBSTITUTE({!Loop_Opps.Phone__c}, &quot; &quot;, &quot;&quot;)
                            )
                        )
                    )
                )
            )
        )
    )
)</expression>
    </formulas>
