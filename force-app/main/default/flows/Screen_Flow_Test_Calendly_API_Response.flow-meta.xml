    <formulas>
        <name>calendlyEventId</name>
        <dataType>String</dataType>
        <expression>TRIM(SUBSTITUTE({!schedule.eventUri}, &quot;https://api.calendly.com/scheduled_events/&quot;, &quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>calendlyInviteeId</name>
        <dataType>String</dataType>
        <expression>RIGHT({!schedule.inviteeUri}, LEN({!schedule.inviteeUri}) - FIND(&quot;/invitees/&quot;, {!schedule.inviteeUri}) - LEN(&quot;/invitees/&quot;) + 1)</expression>
    </formulas>
    <formulas>
        <name>frmEndTime</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(
    LEFT({!Get_Scheduled_Event.end_time}, 10) &amp; &quot; &quot; &amp; 
    MID({!Get_Scheduled_Event.end_time}, 12, 8)
)</expression>
    </formulas>
    <formulas>
        <name>frmStartTime</name>
        <dataType>DateTime</dataType>
