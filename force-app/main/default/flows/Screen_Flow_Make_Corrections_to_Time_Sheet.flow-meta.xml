    <decisions>
        <name>Was_Date_a_Week_Day</name>
        <label>Was Date a Week Day?</label>
        <locationX>380</locationX>
        <locationY>1190</locationY>
        <defaultConnector>
            <targetReference>Screen_3_Weekday_Alert</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Weekend Day Selected</defaultConnectorLabel>
        <rules>
            <name>Date_was_a_Week_Day</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>frmWeekDay</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Sunday</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>frmWeekDay</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Saturday</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_Start_Before_End</targetReference>
            </connector>
            <label>Date was a Week Day</label>
        </rules>
    </decisions>
