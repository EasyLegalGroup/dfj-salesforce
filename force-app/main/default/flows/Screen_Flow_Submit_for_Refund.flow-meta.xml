        <rules>
            <name>Only_1_Order_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>numFoundOrders</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_Single_Order</targetReference>
            </connector>
            <label>Only 1 Order Exists</label>
        </rules>
        <rules>
            <name>X2_or_more_Orders_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>numFoundOrders</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>2.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Screen_2_Select_Order</targetReference>
            </connector>
            <label>2 or more Orders Exists</label>
        </rules>
    </decisions>
