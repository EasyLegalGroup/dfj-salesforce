<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>60.0</apiVersion>
    <description>Migrated from the No Answer -&gt; Missed Appointment process using Status New and Last Cont. No Answer criteria.
If a lead has Lead Status &quot;Working&quot; and Last Contact State &quot;New&quot;, clicking &quot;No answer&quot; will change Last Contact State and Working Status to &quot;Missed Appointment&quot;</description>
    <label>No Answer -&gt; Missed Appointment_1</label>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>myRule_1_A1</name>
        <label>Last Cont. and Working Status = New Appointment</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <inputAssignments>
            <field>Working_Status__c</field>
            <value>
                <stringValue>Missed appoint</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>telegenta__Last_Contact_State__c</field>
            <value>
                <stringValue>Missed Appointment</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>myRule_1_A1</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Status</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Working</stringValue>
            </value>
        </filters>
        <filters>
            <field>telegenta__Last_Contact_State__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>No answer or voicemail</stringValue>
            </value>
        </filters>
        <object>Lead</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Obsolete</status>
</Flow>
