<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>58.0</apiVersion>
    <description>Changed start requirements to be &quot;If Search Google Drive for Journal Number&quot; is null, then proceed.</description>
    <environments>Default</environments>
    <formulas>
        <name>GoogleDriveSearchLink</name>
        <dataType>String</dataType>
        <expression>&quot;https://drive.google.com/drive/u/0/search?q=&quot;&amp;{!$Record.Name}</expression>
    </formulas>
    <interviewLabel>Journal: Set &quot;Google Drive Search Link&quot; field when changed from &quot;New&quot; {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Journal: Set &quot;Google Drive Search Link&quot; field when changed from &quot;New&quot;</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Update_Google_Drive_Search_Field</name>
        <label>Update Google Drive Search Field</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <inputAssignments>
            <field>Search_Google_Drive_for_Journal_Number__c</field>
            <value>
                <elementReference>GoogleDriveSearchLink</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Update_Google_Drive_Search_Field</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Search_Google_Drive_for_Journal_Number__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Journal__c</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Obsolete</status>
</Flow>
