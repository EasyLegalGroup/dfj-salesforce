    <decisions>
        <description>Checks if Leads &quot;Phone Formatted (No Spaces)&quot; matches the Mobile Phone field on Lead. If not, it syncs them</description>
        <name>Does_Phone_and_Mobile_Match</name>
        <label>Does Phone and Mobile Match?</label>
        <locationX>305</locationX>
        <locationY>323</locationY>
        <defaultConnector>
            <targetReference>Sync_Phone_and_Mobile</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Does not Match</defaultConnectorLabel>
        <rules>
            <name>They_Match</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Phone_Formatted_No_Spaces__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Record.MobilePhone</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>myRule_1</targetReference>
            </connector>
            <label>They Match</label>
        </rules>
    </decisions>
