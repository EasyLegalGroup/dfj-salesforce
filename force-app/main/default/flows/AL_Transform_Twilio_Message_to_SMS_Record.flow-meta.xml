    <decisions>
        <name>What_is_the_Country_Code</name>
        <label>What is the Country Code?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Danish_Country_Code</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>TwilioMessage.TwilioSF__From_Number__c</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>+45</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>TwilioMessage.Country_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Denmark</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Danish_Values</targetReference>
            </connector>
            <label>Danish Country Code</label>
        </rules>
        <rules>
            <name>Swedish_Country_Code</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>TwilioMessage.TwilioSF__From_Number__c</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>+46</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>TwilioMessage.Country_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sweden</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Swedish_Values</targetReference>
            </connector>
            <label>Swedish Country Code</label>
        </rules>
    </decisions>
    <decisions>
        <name>What_is_the_Direction</name>
        <label>What is the Direction?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Direction_is_Inbound</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>TwilioMessage.TwilioSF__Direction__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>inbound</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Populate_Inbound_Values</targetReference>
            </connector>
            <label>Direction is Inbound</label>
        </rules>
        <rules>
            <name>Direction_is_Outbound</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>TwilioMessage.TwilioSF__Direction__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>outbound-api</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>TwilioMessage.TwilioSF__Direction__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>outbound-call</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>TwilioMessage.TwilioSF__Direction__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>outbound-reply</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Populate_Outgoing_Values</targetReference>
            </connector>
            <label>Direction is Outbound</label>
        </rules>
    </decisions>
