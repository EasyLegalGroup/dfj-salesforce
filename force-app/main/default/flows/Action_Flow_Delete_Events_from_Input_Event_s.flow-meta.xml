    <decisions>
        <name>Did_we_find_any_Events</name>
        <label>Did we find any Events?</label>
        <locationX>446</locationX>
        <locationY>350</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Events_Found_From_Single</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Events_From_Single</leftValueReference>
                <operator>IsEmpty</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_Event_From_Single_to_Collection</targetReference>
            </connector>
            <label>Events Found (From Single)</label>
        </rules>
    </decisions>
    <decisions>
        <description>If we get a single event, we use it to find all other events and then delete them. If we get a collection, we delete the records in the collection</description>
        <name>Did_We_Get_Single_Event_or_Collection</name>
        <label>Did We Get Single Event or Collection?</label>
        <locationX>666</locationX>
        <locationY>134</locationY>
        <defaultConnector>
            <targetReference>Set_Output_Error_Missing_Entry_Event_s</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Got_both_Single_and_Collection</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EventSingle</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>EventCollection</leftValueReference>
                <operator>IsEmpty</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Error_Cannot_Provide_Both_Single_and_Collection</targetReference>
            </connector>
            <label>Got both Single and Collection</label>
        </rules>
        <rules>
            <name>Single_Event</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EventSingle</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Events_From_Single</targetReference>
            </connector>
            <label>Got Single Event</label>
        </rules>
        <rules>
            <name>Got_Event_Collection</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EventCollection</leftValueReference>
                <operator>EqualTo</operator>
            </conditions>
            <connector>
                <targetReference>Delete_Events</targetReference>
            </connector>
            <label>Got Event Collection</label>
        </rules>
    </decisions>
    <environments>Default</environments>
