    <formulas>
        <name>frmCleanFromNumber</name>
        <dataType>String</dataType>
        <expression>IF(
  BEGINS({!frmCleanRawFromNumber},&quot;00&quot;),
  &quot;+&quot; &amp; RIGHT({!frmCleanRawFromNumber}, LEN({!frmCleanRawFromNumber})-2),
  {!frmCleanRawFromNumber}
)</expression>
    </formulas>
    <formulas>
        <name>frmCleanRawFromNumber</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(
  SUBSTITUTE(
    SUBSTITUTE(
      SUBSTITUTE(
        TRIM({!$Record.From_Number__c}),
      &quot; &quot;,&quot;&quot;),
    &quot;(&quot;,&quot;&quot;),
  &quot;)&quot;,&quot;&quot;),
&quot;-&quot;,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>frmCleanRawToNumber</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(
  SUBSTITUTE(
