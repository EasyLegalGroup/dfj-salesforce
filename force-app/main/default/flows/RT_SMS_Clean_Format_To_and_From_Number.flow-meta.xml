&quot;-&quot;,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>frmCleanToNumber</name>
        <dataType>String</dataType>
        <expression>IF(
  BEGINS({!frmCleanRawToNumber},&quot;00&quot;),
  &quot;+&quot; &amp; RIGHT({!frmCleanRawToNumber}, LEN({!frmCleanRawToNumber})-2),
  {!frmCleanRawToNumber}
)</expression>
    </formulas>
    <formulas>
        <name>frmFormattedFromNumber</name>
        <dataType>String</dataType>
        <expression>IF(
  OR(
    BEGINS({!frmCleanFromNumber},&quot;+45&quot;),
    BEGINS({!frmCleanFromNumber},&quot;+46&quot;),
    BEGINS({!frmCleanFromNumber},&quot;+353&quot;)
  ),
  {!frmCleanFromNumber},

  IF(
    OR(
      BEGINS({!frmCleanFromNumber},&quot;45&quot;),
      BEGINS({!frmCleanFromNumber},&quot;46&quot;),
      BEGINS({!frmCleanFromNumber},&quot;353&quot;)
    ),
    &quot;+&quot; &amp; {!frmCleanFromNumber},

    IF(
      AND(
        TEXT({!$Record.Market_Unit__c})=&quot;DFJ_DK&quot;,
        NOT( OR(BEGINS({!frmCleanFromNumber},&quot;+&quot;), BEGINS({!frmCleanFromNumber},&quot;45&quot;)) )
      ),
      &quot;+45&quot; &amp; {!frmCleanFromNumber},

      IF(
        AND(
          TEXT({!$Record.Market_Unit__c})=&quot;FA_SE&quot;,
          NOT( OR(BEGINS({!frmCleanFromNumber},&quot;+&quot;), BEGINS({!frmCleanFromNumber},&quot;46&quot;)) )
        ),
        &quot;+46&quot; &amp; {!frmCleanFromNumber},

        IF(
          AND(
            TEXT({!$Record.Market_Unit__c})=&quot;Ireland&quot;,
            NOT( OR(BEGINS({!frmCleanFromNumber},&quot;+&quot;), BEGINS({!frmCleanFromNumber},&quot;353&quot;)) )
          ),
          &quot;+353&quot; &amp; {!frmCleanFromNumber},

          IF(
            BEGINS({!frmCleanFromNumber},&quot;+&quot;),
            {!frmCleanFromNumber},
            &quot;+&quot; &amp; {!frmCleanFromNumber}
          )
        )
      )
    )
  )
)</expression>
    </formulas>
    <formulas>
        <name>frmFormattedToNumber</name>
        <dataType>String</dataType>
        <expression>IF(
  OR(
    BEGINS({!frmCleanToNumber},&quot;+45&quot;),
    BEGINS({!frmCleanToNumber},&quot;+46&quot;),
