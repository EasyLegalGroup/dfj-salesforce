    <formulas>
        <name>frmCampaignName</name>
        <dataType>String</dataType>
        <expression>{!Campaign.Account__r.Name} &amp; &quot; - &quot; &amp;
RIGHT(&quot;0&quot; &amp; TEXT(DAY(DATEVALUE({!inputStart}))), 2) &amp; &quot;-&quot; &amp;
RIGHT(&quot;0&quot; &amp; TEXT(MONTH(DATEVALUE({!inputStart}))), 2) &amp; &quot;-&quot; &amp;
TEXT(YEAR(DATEVALUE({!inputStart})))</expression>
    </formulas>
    <formulas>
        <name>frmEnd</name>
        <dataType>DateTime</dataType>
        <expression>{!inputStart} + (3/24)</expression>
    </formulas>
    <formulas>
        <name>frmIncrementedRelatedEventId</name>
        <dataType>String</dataType>
        <expression>LEFT({!Get_Campaign.Related_Event_ID__c}, FIND(&quot;-&quot;, {!Get_Campaign.Related_Event_ID__c}, FIND(&quot;-&quot;, {!Get_Campaign.Related_Event_ID__c}) + 1))
&amp; 
TEXT(
  VALUE(
    RIGHT(
      {!Get_Campaign.Related_Event_ID__c},
      LEN({!Get_Campaign.Related_Event_ID__c}) - FIND(&quot;-&quot;, {!Get_Campaign.Related_Event_ID__c}, FIND(&quot;-&quot;, {!Get_Campaign.Related_Event_ID__c}) + 1)
    )
  ) + 1
)</expression>
    </formulas>
