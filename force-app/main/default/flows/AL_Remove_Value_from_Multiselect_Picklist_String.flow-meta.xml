    <decisions>
        <name>Is_Value_Present</name>
        <label>Is Value Present?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Populate_Custom_Error_with_No_Matching_Active_Campaign_Session</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Value_is_Present</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>frmIsValuePresent</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Populate_Success_Values</targetReference>
            </connector>
            <label>Value is Present</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>frmIsValuePresent</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS(
  &quot;;&quot; &amp; {!inputActiveCallCampaigns} &amp; &quot;;&quot;,
  &quot;;&quot; &amp; {!frmTrimmedCampaignName} &amp; &quot;;&quot;
)</expression>
    </formulas>
    <formulas>
        <name>frmRemoveCallCampaignValue</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(
  &quot;;&quot; &amp; {!inputActiveCallCampaigns} &amp; &quot;;&quot;,
  &quot;;&quot; &amp; {!frmTrimmedCampaignName} &amp; &quot;;&quot;,
  &quot;;&quot;
)</expression>
    </formulas>
    <formulas>
        <name>frmTrimLeftSemicolon</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!frmRemoveCallCampaignValue},1)=&quot;;&quot;,
   MID({!frmRemoveCallCampaignValue},2,LEN({!frmRemoveCallCampaignValue})-1),
   {!frmRemoveCallCampaignValue}
)</expression>
    </formulas>
    <formulas>
        <name>frmTrimmedCampaignName</name>
        <dataType>String</dataType>
        <expression>IF(
   CONTAINS({!Get_Campaign_Session.Call_Campaign_Name__c}, &quot; (Final)&quot;),
   SUBSTITUTE({!Get_Campaign_Session.Call_Campaign_Name__c}, &quot; (Final)&quot;, &quot;&quot;),
   IF(
      CONTAINS({!Get_Campaign_Session.Call_Campaign_Name__c}, &quot; (Initial)&quot;),
      SUBSTITUTE({!Get_Campaign_Session.Call_Campaign_Name__c}, &quot; (Initial)&quot;, &quot;&quot;),
      {!Get_Campaign_Session.Call_Campaign_Name__c}
   )
)</expression>
    </formulas>
    <formulas>
        <name>frmTrimRightSemicolon</name>
        <dataType>String</dataType>
        <expression>IF(RIGHT({!frmTrimLeftSemicolon},1)=&quot;;&quot;,
   LEFT({!frmTrimLeftSemicolon}, LEN({!frmTrimLeftSemicolon})-1),
   {!frmTrimLeftSemicolon}
)</expression>
    </formulas>
    <interviewLabel>AL - Remove Call Campaign from Multiselect Picklist Value {!$Flow.CurrentDateTime}</interviewLabel>
