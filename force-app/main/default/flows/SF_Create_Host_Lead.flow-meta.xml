            <name>Got_Duplicates</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>DC_Find_Duplicate_by_Field_Values_Action.duplicate</leftValueReference>
                <operator>EqualTo</operator>
