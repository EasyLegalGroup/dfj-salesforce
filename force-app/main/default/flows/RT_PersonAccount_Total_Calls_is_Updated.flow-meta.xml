            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Working_Status__pc</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Missed Appointment</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Total_Calls__pc</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>20.0</numberValue>
                </rightValue>
