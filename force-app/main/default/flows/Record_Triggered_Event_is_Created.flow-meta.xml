    <description>Added populating of new &quot;Subject&quot; field on Appointment.</description>
    <environments>Default</environments>
    <formulas>
        <name>ExtractedRelatedEventId</name>
        <dataType>String</dataType>
        <expression>TRIM(
    LEFT(
        MID(
            {!Description},
            FIND(&quot;Event ID: &quot;, {!Description}) + 10,
            14
        ),
        14
    )
)</expression>
    </formulas>
    <formulas>
        <name>SubjectExtractedRelatedEventId</name>
        <dataType>String</dataType>
        <expression>MID({!$Record.Subject}, FIND(&quot;#&quot;, {!$Record.Subject}) + 1, LEN({!$Record.Subject}))</expression>
    </formulas>
    <formulas>
        <name>TrimmedEventId</name>
        <dataType>String</dataType>
        <expression>LEFT({!RelatedEventId}, LEN({!RelatedEventId}) - 1)</expression>
    </formulas>
    <formulas>
        <name>TrimmedSubject</name>
        <dataType>String</dataType>
        <expression>IF(
  LEN({!$Record.Subject}) &gt; 80,
  LEFT({!$Record.Subject}, 77) &amp; &quot;...&quot;,
  {!$Record.Subject}
)</expression>
    </formulas>
