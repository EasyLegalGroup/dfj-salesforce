    </formulas>
    <formulas>
        <name>frmNamePrefix</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!plProjectType}),
  &quot;Project: &quot;,
  IF(
    {!plProjectType} = &quot;Child_Project&quot;,
    &quot;Child Project: &quot;,
    IF(
      {!plProjectType} = &quot;Parent_Project&quot;,
      &quot;Parent Project: &quot;,
      IF(
        {!plProjectType} = &quot;Standalone_Project&quot;,
        &quot;Standalone Project: &quot;,
        &quot;Project: &quot;
      )
    )
  )
)</expression>
    </formulas>
    <formulas>
        <name>frmShowParentBoolean</name>
        <dataType>Boolean</dataType>
        <expression>IF({!plProjectType} = &quot;Child_Project&quot;, false, true)</expression>
    </formulas>
