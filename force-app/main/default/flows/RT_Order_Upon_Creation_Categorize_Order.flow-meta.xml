    <formulas>
        <name>fr_AddToAegtepagt</name>
        <dataType>Number</dataType>
        <expression>IF(
  {!Loop_Order_Products.Product2.Product_Identifier__c} = &quot;AP&quot;, 
 {!Loop_Order_Products.Quantity}, 
  0
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>This formula accepts both FF and FFV, and returns Quantity if either is true.</description>
        <name>fr_AddToFremtidsfuldmagt</name>
        <dataType>Number</dataType>
        <expression>IF(
  OR(
    {!Loop_Order_Products.Product2.Product_Identifier__c} = &quot;FF&quot;,
    {!Loop_Order_Products.Product2.Product_Identifier__c} = &quot;FFV&quot;
  ),
  {!Loop_Order_Products.Quantity},
  0
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fr_AddToGeneralfuldmagt</name>
        <dataType>Number</dataType>
        <expression>IF(
  {!Loop_Order_Products.Product2.Product_Identifier__c} = &quot;GF&quot;, 
 {!Loop_Order_Products.Quantity}, 
  0
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fr_AddToTestamente</name>
        <dataType>Number</dataType>
        <expression>IF(
  OR(
    {!Loop_Order_Products.Product2.Product_Identifier__c} = &quot;TS&quot;, 
    {!Loop_Order_Products.Product2.Product_Identifier__c} = &quot;30T&quot;
  ),
  {!Loop_Order_Products.Quantity},
  0
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fr_SkipCategorization</name>
        <dataType>Number</dataType>
        <expression>IF({!Loop_Order_Products.Product2.Skip_Categorization__c}, 1, 0)</expression>
        <scale>0</scale>
    </formulas>
