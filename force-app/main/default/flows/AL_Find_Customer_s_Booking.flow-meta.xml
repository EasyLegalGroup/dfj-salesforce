<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>63.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Assign_Campaign_Member_ID_to_Output_Variable</name>
        <label>Assign Campaign Member ID to Output Variable</label>
        <locationX>50</locationX>
        <locationY>350</locationY>
        <assignmentItems>
            <assignToReference>outputCampaignMemberId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>CampaignMember.Id</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>outputEmailAddress</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>CampaignMember.Email</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>outputPhone</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>CampaignMember.Phone_Formatted__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>outputZipCode</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>CampaignMember.Host_Address_Zip_Code__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>outputFullName</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>CampaignMember.Name</elementReference>
            </value>
        </assignmentItems>
    </assignments>
    <assignments>
        <name>Set_Fault_Outcome_to_No_Booking_Found</name>
        <label>Set Fault Outcome to No Booking Found</label>
        <locationX>314</locationX>
        <locationY>350</locationY>
        <assignmentItems>
            <assignToReference>outputFaultOutcome</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>No Booking Found</stringValue>
            </value>
        </assignmentItems>
    </assignments>
    <decisions>
        <name>Did_We_Find_a_Booking</name>
        <label>Did We Find a Booking?</label>
        <locationX>182</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>Set_Fault_Outcome_to_No_Booking_Found</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Got_Booking</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CampaignMember.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Campaign_Member_ID_to_Output_Variable</targetReference>
            </connector>
            <label>Got Booking</label>
        </rules>
    </decisions>
    <description>This flow is used by an Agentforce agent, to find a customer&apos;s booking based on External ID (known to the customer as Booking Number) and output certain values to the Agent.</description>
    <environments>Default</environments>
    <interviewLabel>Agentforce: Find Booking {!$Flow.CurrentDateTime}</interviewLabel>
    <label>AL - Find Customer&apos;s Booking</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <description>Finds a Campaign Member based on an External ID provided by the customer. The Campaign Member is the customer&apos;s booking.</description>
        <name>Get_Campaign_Member</name>
        <label>Get Campaign Member</label>
        <locationX>182</locationX>
        <locationY>134</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Did_We_Find_a_Booking</targetReference>
        </connector>
        <filterLogic>(1 OR 2) AND 3</filterLogic>
        <filters>
            <field>External_ID__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>inputExternalId</elementReference>
            </value>
        </filters>
        <filters>
            <field>Email</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>inputEmail</elementReference>
            </value>
        </filters>
        <filters>
            <field>Event_Start__c</field>
            <operator>GreaterThanOrEqualTo</operator>
            <value>
                <elementReference>$Flow.CurrentDate</elementReference>
            </value>
        </filters>
        <object>CampaignMember</object>
        <outputReference>CampaignMember</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>Name</queriedFields>
        <queriedFields>Email</queriedFields>
        <queriedFields>Phone_Formatted__c</queriedFields>
        <queriedFields>Venue__c</queriedFields>
        <queriedFields>Event_Start__c</queriedFields>
        <queriedFields>Host_Address_Street__c</queriedFields>
        <queriedFields>Host_Address_Zip_Code__c</queriedFields>
        <queriedFields>Host_Address_City__c</queriedFields>
        <queriedFields>LeadOrContactId</queriedFields>
        <queriedFields>Remaining_Tickets__c</queriedFields>
        <sortField>Event_Start__c</sortField>
        <sortOrder>Desc</sortOrder>
    </recordLookups>
    <runInMode>SystemModeWithoutSharing</runInMode>
    <start>
        <locationX>56</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_Campaign_Member</targetReference>
        </connector>
    </start>
    <status>Active</status>
    <variables>
        <description>This is the customer&apos;s booking</description>
        <name>CampaignMember</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>true</isOutput>
        <objectType>CampaignMember</objectType>
    </variables>
    <variables>
        <name>inputEmail</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <description>External ID is known to the customer as their Booking Number. This is what we ask them for, to find their booking.</description>
        <name>inputExternalId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <name>outputCampaignMemberId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>outputEmailAddress</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>outputFaultOutcome</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>outputFullName</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>outputPhone</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>outputZipCode</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>true</isOutput>
    </variables>
</Flow>
