    <formulas>
        <name>frmCampaignName</name>
        <dataType>String</dataType>
        <expression>{!inputHostRecord.Name} &amp; &quot; - &quot; &amp; {!frmStartPretty}</expression>
    </formulas>
    <formulas>
        <name>frmEndDateTime</name>
        <dataType>DateTime</dataType>
        <expression>CASE(
    TEXT({!inputHostRecord.Market_Unit__c}),
    &quot;DFJ_DK&quot;, {!inputCalendlyAction.Calendly__EventStartTime__c} + (3/24),
    &quot;Testamente_DK&quot;, {!inputCalendlyAction.Calendly__EventStartTime__c} + (3/24),
    &quot;FA_SE&quot;, {!inputCalendlyAction.Calendly__EventStartTime__c} + (2/24),
    &quot;Testamente_SV&quot;, {!inputCalendlyAction.Calendly__EventStartTime__c} + (2/24),
    {!inputCalendlyAction.Calendly__EventStartTime__c}
)</expression>
    </formulas>
    <formulas>
        <name>frmStartPretty</name>
        <dataType>String</dataType>
        <expression>TEXT(DAY(DATEVALUE({!inputCalendlyAction.Calendly__EventStartTime__c}))) &amp; &quot;-&quot; &amp; 
LPAD(TEXT(MONTH(DATEVALUE({!inputCalendlyAction.Calendly__EventStartTime__c}))), 2, &quot;0&quot;) &amp; &quot;-&quot; &amp; 
TEXT(YEAR(DATEVALUE({!inputCalendlyAction.Calendly__EventStartTime__c})))</expression>
    </formulas>
