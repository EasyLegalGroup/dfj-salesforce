    <formulas>
        <name>BaseQty</name>
        <dataType>Number</dataType>
        <expression>CASE(
 {!Loop_Current_Discounts.Discount_Related_To__r.Product_Identifier__c},
  &quot;AP&quot;,  {!QtyAegtepagt},
  &quot;FF&quot;,  {!QtyFremtidsfuldmagt},
