    <formulas>
        <name>frmPhoneFormatted</name>
        <dataType>String</dataType>
        <expression>IF(
    BEGINS(SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;), &apos;+45&apos;),
    SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;),
    IF(
        BEGINS(SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;), &apos;45&apos;),
        &apos;+&apos; + SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;),
        IF(
            AND(
                NOT(BEGINS(SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;), &apos;+45&apos;)),
                OR(
                    {!inputMarketUnit} = &apos;DFJ_DK&apos;,
                    {!inputMarketUnit} = &apos;Testamente_DK&apos;
                )
            ),
            &apos;+45&apos; + SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;),
            IF(
                AND(
                    NOT(BEGINS(SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;), &apos;+&apos;)),
                    OR(
                        {!inputMarketUnit} = &apos;DFJ_DK&apos;,
                        {!inputMarketUnit} = &apos;Testamente_DK&apos;
                    )
                ),
                &apos;+&apos; + SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;),
                IF(
                    BEGINS(SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;), &apos;+46&apos;),
                    SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;),
                    IF(
                        BEGINS(SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;), &apos;46&apos;),
                        &apos;+&apos; + SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;),
                        IF(
                            AND(
                                NOT(BEGINS(SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;), &apos;+46&apos;)),
                                {!inputMarketUnit} = &apos;FA_SE&apos;
                            ),
                            &apos;+46&apos; + SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;),
                            IF(
                                AND(
                                    NOT(BEGINS(SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;), &apos;+&apos;)),
                                    {!inputMarketUnit} = &apos;FA_SE&apos;
                                ),
                                &apos;+&apos; + SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;),
                                SUBSTITUTE({!inputPhone}, &quot; &quot;, &quot;&quot;)
                            )
                        )
                    )
                )
            )
        )
    )
)</expression>
    </formulas>
