    <formulas>
        <name>frmDummyEmail</name>
        <dataType>String</dataType>
        <expression>&quot;test_&quot; &amp;
RIGHT(
  &quot;0000000000&quot; &amp;
  TEXT(
    MOD(
      (FLOOR( ($Flow.CurrentDateTime - DATETIMEVALUE(&quot;1970-01-01 00:00:00Z&quot;)) * 86400 ) * 1664525 + 1013904223),
      1000000000
    )
  ),
  9
) &amp;
&quot;@example.com&quot;</expression>
    </formulas>
    <formulas>
        <name>frmDummyPhone</name>
        <dataType>String</dataType>
        <expression>&quot;+45&quot; &amp;
RIGHT(
  &quot;00000000&quot; &amp;
  TEXT(
    MOD(
      (FLOOR( ($Flow.CurrentDateTime - DATETIMEVALUE(&quot;1970-01-01 00:00:00Z&quot;)) * 86400 ) * 1103515245 + 12345),
      100000000
    )
  ),
  8
)</expression>
    </formulas>
    <formulas>
        <name>frmIdentifierValue</name>
        <dataType>String</dataType>
        <expression>IF(
  {!plSelectSearchParameter} = &quot;Email&quot;,
  {!txtTemporaryAccountEmail},
  IF(
    {!plSelectSearchParameter} = &quot;Phone&quot;,
    {!txtTempAccountPhone},
    &quot;&quot;
  )
)</expression>
    </formulas>
