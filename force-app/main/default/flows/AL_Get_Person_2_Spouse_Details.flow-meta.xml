    <formulas>
        <name>frmSpousePhoneFormatted</name>
        <dataType>String</dataType>
        <expression>IF(
BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+45&apos;),
SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;45&apos;),
&apos;+&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
AND(
NOT(BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+45&apos;)),
OR(
ISPICKVAL({!inputJournal.Market_Unit__c}, &apos;DFJ_DK&apos;),
ISPICKVAL({!inputJournal.Market_Unit__c}, &apos;Testamente_DK&apos;)
)
),
&apos;+45&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
AND(
NOT(BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+&apos;)),
OR(
ISPICKVAL({!inputJournal.Market_Unit__c}, &apos;DFJ_DK&apos;),
ISPICKVAL({!inputJournal.Market_Unit__c}, &apos;Testamente_DK&apos;)
)
),
&apos;+&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+46&apos;),
SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;46&apos;),
&apos;+&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
AND(
NOT(BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+46&apos;)),
ISPICKVAL({!inputJournal.Market_Unit__c}, &apos;FA_SE&apos;)
),
&apos;+46&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
AND(
NOT(BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+&apos;)),
ISPICKVAL({!inputJournal.Market_Unit__c}, &apos;FA_SE&apos;)
),
&apos;+&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
/* --- Ireland block starts --- */
IF(
BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+353&apos;),
SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;353&apos;),
&apos;+&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
AND(
NOT(BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+353&apos;)),
ISPICKVAL({!inputJournal.Market_Unit__c}, &apos;Ireland&apos;)
),
&apos;+353&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
IF(
AND(
NOT(BEGINS(SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;), &apos;+&apos;)),
ISPICKVAL({!inputJournal.Market_Unit__c}, &apos;Ireland&apos;)
),
&apos;+&apos; + SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;),
SUBSTITUTE({!SpousePhone}, &quot; &quot;, &quot;&quot;)
)
)
)
)
/* --- Ireland block ends --- */
)
)
)
)
)
)
)
)</expression>
