    <decisions>
        <name>Determine_Call_Priority</name>
        <label>Determine Call Priority</label>
        <locationX>930</locationX>
        <locationY>287</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>X60_Lead_was_Reopened</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Gen_bnet__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Call_Priority_to_60</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>60 - Lead was Reopened</label>
        </rules>
        <rules>
            <name>X90_Got_Appointment_With_owner_Not_Ring_Igen</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.telegenta__Last_Contact_State__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Got appointment</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OwnerId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>005</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Rating</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Ring igen</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Call_Priority_to_90</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>90 - Got Appointment (With owner, Not Ring Igen)</label>
        </rules>
        <rules>
            <name>X80_Internal_Booked_Meeting</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Working</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OwnerId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>005</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.M_de_booket__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Call_Priority_to_80</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>80 - Internal Booked Meeting</label>
        </rules>
        <rules>
            <name>X70_Got_Appointment_No_owner</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.telegenta__Last_Contact_State__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Got appointment</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OwnerId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>00G</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Call_Priority_to_70</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>70 - Got Appointment (No owner)</label>
        </rules>
        <rules>
            <name>X30_Missed_Released_Ring_Igen</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>$Record.telegenta__Last_Contact_State__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Missed Appointment</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.telegenta__Last_Contact_State__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Released</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Rating</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Ring igen</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Call_Priority_to_30</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>30 - Missed / Released / Ring Igen</label>
        </rules>
        <rules>
            <name>X20_No_Answer_Busy</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.telegenta__Last_Contact_State__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>No answer or voicemail</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.telegenta__Last_Contact_State__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Number busy</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Call_Priority_to_20</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>20 - No Answer / Busy</label>
        </rules>
