        </defaultConnector>
        <defaultConnectorLabel>Not Awaiting Customer</defaultConnectorLabel>
        <rules>
            <name>Already_Awaiting_Customer</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Journal.GA_Reply_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Awaiting customer</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Clear_Awaiting_Agent_Since</targetReference>
            </connector>
            <label>Already Awaiting Customer</label>
        </rules>
    </decisions>
