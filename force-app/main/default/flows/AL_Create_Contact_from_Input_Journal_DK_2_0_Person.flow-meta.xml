    <decisions>
        <description>Checks if it&apos;s either Person 1 or Person 2, that membership is checked and that Journal has an Account</description>
        <name>Is_Person_Valid_for_CPR_Subscription</name>
        <label>Is Person Valid for CPR Subscription?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Populate_Non_CPR_Variables</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not a Valid Subscriber</defaultConnectorLabel>
        <rules>
            <name>Is_Valid_Person</name>
            <conditionLogic>(1 OR 2) AND 3 AND 4 AND 5</conditionLogic>
            <conditions>
                <leftValueReference>inputPerson.person_hidden_person_1__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>inputPerson.person_hidden_person_2__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>inputPerson.person_medlem_dfj__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>inputPerson.journal_dk_new__r.Journal__r.Account__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>inputPerson</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Populate_CPR_Variables</targetReference>
            </connector>
            <label>Is Valid Person</label>
        </rules>
    </decisions>
    <description>This flow creates a contact and sets &quot;Is CPR Subscriber&quot; to true, if it matches the criteria (Journal has a related Account, Person is marked as member, has Person 1 or Person 2 true, etc.)</description>
    <environments>Default</environments>
    <formulas>
        <name>frmBusinessPhone</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!inputPerson.person_telefonnummer__c}),
  IF(
    NOT(ISBLANK({!Contact.Phone})),
    {!Contact.Phone},
    &quot;&quot;
  ),
  {!inputPerson.person_telefonnummer__c}
)</expression>
    </formulas>
    <formulas>
        <name>frmFirstName</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!inputPerson.person_fornavn__c}),
  IF(
    NOT(ISBLANK({!Contact.FirstName})),
    {!Contact.FirstName},
    &quot;UNKNOWN FIRST NAME&quot;
  ),
  IF(
    LEN({!inputPerson.person_fornavn__c}) &lt;= 40,
    {!inputPerson.person_fornavn__c},
    LEFT({!inputPerson.person_fornavn__c}, 37) &amp; &quot;...&quot;
  )
)</expression>
    </formulas>
    <formulas>
        <name>frmLastName</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!inputPerson.person_efternavn__c}),
  IF(
    NOT(ISBLANK({!Contact.LastName})),
    {!Contact.LastName},
    &quot;UNKNOWN LAST NAME&quot;
  ),
  {!inputPerson.person_efternavn__c}
)</expression>
    </formulas>
    <formulas>
        <name>frmMobilePhone</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!inputPerson.person_telefonnummer__c}),
  IF(
    NOT(ISBLANK({!Contact.MobilePhone})),
    {!Contact.MobilePhone},
    &quot;&quot;
  ),
  {!inputPerson.person_telefonnummer__c}
)</expression>
    </formulas>
    <formulas>
        <name>IsEmailValid</name>
        <dataType>Boolean</dataType>
        <expression>REGEX({!inputPerson.person_email__c}, &quot;^[a-zA-Z0-9._%+-]+@[a-zA-Z0-9-]+(\\.[a-zA-Z0-9-]+)*\\.[a-zA-Z]{2,4}$&quot;)</expression>
    </formulas>
