<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>62.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Count_Calls_Since_Last_Reopen</name>
        <label>Count Calls Since Last Reopen</label>
        <locationX>182</locationX>
        <locationY>503</locationY>
        <assignmentItems>
            <assignToReference>numCallsSinceLastReopen</assignToReference>
            <operator>AssignCount</operator>
            <value>
                <elementReference>Get_Telegenta_Call_Histories</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>How_Many_Calls_Since_Last_Reopen</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Count_Calls_and_Determine_Outcome</name>
        <label>Count Calls and Determine Outcome</label>
        <locationX>336</locationX>
        <locationY>1103</locationY>
        <defaultConnector>
            <targetReference>Do_we_have_Calls_Since_Last_Reopen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Invalidate_Lead</name>
            <conditionLogic>(1 AND 2 AND 3) OR (4 AND 5 AND 6)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Total_calls_out_all__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>16.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Gen_bnet__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Total_calls_out_all__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>8.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Gen_bnet__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Lead_with_Invalid_Values</targetReference>
            </connector>
            <label>Invalidate Lead</label>
        </rules>
        <rules>
            <name>Close_Lead</name>
            <conditionLogic>1 AND 2 AND 3 AND 4</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Working_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Missed appoint</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Working_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Missed appointment (next call time)</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Total_calls_out_all__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>20.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Lead_with_Closed_Values</targetReference>
            </connector>
            <label>Close Lead</label>
        </rules>
    </decisions>
    <decisions>
        <name>Do_we_have_Calls_Since_Last_Reopen</name>
        <label>Do we have Calls Since Last Reopen?</label>
        <locationX>336</locationX>
        <locationY>1403</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Got_Calls_Since_Last_Reopen</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>numCallsSinceLastReopen</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <isGoTo>true</isGoTo>
                <targetReference>Update_Lead_with_Calls_Since_Last_Reopen</targetReference>
            </connector>
            <label>Got Calls Since Last Reopen</label>
        </rules>
    </decisions>
    <decisions>
        <name>How_Many_Calls_Since_Last_Reopen</name>
        <label>How Many Calls Since Last Reopen?</label>
        <locationX>182</locationX>
        <locationY>611</locationY>
        <defaultConnector>
            <targetReference>Count_Calls_and_Determine_Outcome</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Fewer_than_8</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>numCallsSinceLastReopen</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>8.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Lead_with_Calls_Since_Last_Reopen</targetReference>
            </connector>
            <label>Fewer than 8</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Last_Reopened_Date_Set</name>
        <label>Is Last Reopened Date Set?</label>
        <locationX>336</locationX>
        <locationY>287</locationY>
        <defaultConnector>
            <targetReference>Count_Calls_and_Determine_Outcome</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Got_Last_Reopened_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Last_Reopened_Date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Telegenta_Call_Histories</targetReference>
            </connector>
            <label>Got Last Reopened Date</label>
        </rules>
    </decisions>
    <description>Whenever the value in the field &quot;Total Calls Out All&quot; changes, this flow runs. It then checks how many calls were performed and what status the Lead was in and if e.g. a certain amount of calls is reached on a Lead with a particular status, it will then invalidate or close the Lead</description>
    <environments>Default</environments>
    <interviewLabel>AS - Lead: Total Calls Out All Changed {!$Flow.CurrentDateTime}</interviewLabel>
    <label>BS - Lead: Total Calls Out All Changed</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Get_Telegenta_Call_Histories</name>
        <label>Get Telegenta Call Histories</label>
        <locationX>182</locationX>
        <locationY>395</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Count_Calls_Since_Last_Reopen</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>telegenta__Lead__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <filters>
            <field>CreatedDate</field>
            <operator>GreaterThanOrEqualTo</operator>
            <value>
                <elementReference>$Record.Last_Reopened_Date__c</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>false</getFirstRecordOnly>
        <object>telegenta__Telegenta_Call_History__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordUpdates>
        <name>Update_Lead_with_Calls_Since_Last_Reopen</name>
        <label>Update Lead with Calls Since Last Reopen</label>
        <locationX>50</locationX>
        <locationY>719</locationY>
        <inputAssignments>
            <field>Calls_Since_Last_Reopen__c</field>
            <value>
                <elementReference>numCallsSinceLastReopen</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Update_Lead_with_Closed_Values</name>
        <label>Update Lead with Closed Values</label>
        <locationX>336</locationX>
        <locationY>1211</locationY>
        <connector>
            <targetReference>Do_we_have_Calls_Since_Last_Reopen</targetReference>
        </connector>
        <inputAssignments>
            <field>Closed_Not_Converted_Status__c</field>
            <value>
                <stringValue>Not interested</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Status</field>
            <value>
                <stringValue>Closed - Not Converted</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>telegenta__Not_Interested_Reason__c</field>
            <value>
                <stringValue>Besvarer ikke l√¶ngere</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Update_Lead_with_Invalid_Values</name>
        <label>Update Lead with Invalid Values</label>
        <locationX>72</locationX>
        <locationY>1211</locationY>
        <connector>
            <targetReference>Do_we_have_Calls_Since_Last_Reopen</targetReference>
        </connector>
        <inputAssignments>
            <field>Status</field>
            <value>
                <stringValue>Unqualified</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Unqualified_Status__c</field>
            <value>
                <stringValue>Maxcall</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>telegenta__Invalid_Reason__c</field>
            <value>
                <stringValue>MAXCALL</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>210</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Is_Last_Reopened_Date_Set</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Total_calls_out_all__c</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Lead</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
    <variables>
        <name>numCallsSinceLastReopen</name>
        <dataType>Number</dataType>
        <isCollection>false</isCollection>
