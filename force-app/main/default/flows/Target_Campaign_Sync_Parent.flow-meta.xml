    <decisions>
        <name>Does_Dates_Match</name>
        <label>Does Dates Match?</label>
        <locationX>50</locationX>
        <locationY>360</locationY>
        <defaultConnector>
            <targetReference>Get_Parent</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Dates_Match</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Parent.StartDate</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Start__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Parent.EndDate</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Start__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Parent.Market_Unit__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Market_Unit__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Parent.RecordTypeId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>012W5000000rBRxIAM</stringValue>
                </rightValue>
            </conditions>
            <label>Dates Match</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_there_a_Parent</name>
        <label>Is there a Parent?</label>
        <locationX>204</locationX>
        <locationY>252</locationY>
        <defaultConnector>
            <targetReference>Get_Parent</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Find Parent</defaultConnectorLabel>
        <rules>
            <name>Parent_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ParentId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Does_Dates_Match</targetReference>
            </connector>
            <label>Parent Exists</label>
        </rules>
    </decisions>
