    <formulas>
        <name>firstName</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!$Record.person_fornavn__c}),
  &quot;UNKNOWN FIRST NAME&quot;,
  IF(
    LEN({!$Record.person_fornavn__c}) &lt;= 40,
    {!$Record.person_fornavn__c},
    LEFT({!$Record.person_fornavn__c}, 37) &amp; &quot;...&quot;
  )
)</expression>
    </formulas>
    <formulas>
