    <decisions>
        <name>Check_lead_Foredragsdato_and_update_lead</name>
        <label>Check lead Foredragsdato and update lead</label>
        <locationX>1106</locationX>
        <locationY>2897</locationY>
        <defaultConnector>
            <targetReference>Was_Lead_Closed</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>need_to_update_lead</name>
            <conditionLogic>1 AND 2 AND (3 OR 4) AND (5 OR 6)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Zapier_Flag__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.UTM_Source__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Foredrag</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Foredragstid_og_dato__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Foredragsdato__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Foredragstid_og_dato__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.Foredragstid_og_dato__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Match_Foredragsdato_to_Foredragstid_og_dato</targetReference>
            </connector>
            <label>need to update lead</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_Lead_Status</name>
        <label>Check Lead Status</label>
        <locationX>1106</locationX>
        <locationY>1997</locationY>
        <defaultConnector>
            <targetReference>IsNull_MobilePhone</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Working</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Working</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.telegenta__Last_Contact_State__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>No answer or voicemail</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.Status</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Last_Cont_and_Working_Status_New_Appointment</targetReference>
            </connector>
            <label>Working</label>
        </rules>
        <rules>
            <name>Not_Working</name>
            <conditionLogic>(1 OR 2 OR 3) AND 4</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Unqualified</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed - Not Converted</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Converted</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.Status</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_Last_Cont_and_Working_Status_New_Appointment</targetReference>
            </connector>
            <label>Not Working</label>
        </rules>
    </decisions>
