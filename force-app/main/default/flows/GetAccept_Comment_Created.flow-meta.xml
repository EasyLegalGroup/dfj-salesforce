            <connector>
                <targetReference>Assign_Journal_to_Comment</targetReference>
            </connector>
            <label>Journal Exists on Document</label>
        </rules>
    </decisions>
    <decisions>
        <description>Checks if the message contains the {{recipient.first_name}} merge tag and skips the below actions, if it was</description>
        <name>Was_it_an_automated_chat_message</name>
        <label>Was it an automated chat message?</label>
        <locationX>204</locationX>
        <locationY>923</locationY>
        <defaultConnector>
            <targetReference>Get_Document</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Comment_was_Conversation_Starter</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.GetAccept__CommentText__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>{{recipient.first_name}}</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_GetAccept_Comment</targetReference>
            </connector>
            <label>Comment was Conversation Starter</label>
        </rules>
    </decisions>
