            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Journal__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.GetAccept__GetAccept_Document__r.Journal__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
