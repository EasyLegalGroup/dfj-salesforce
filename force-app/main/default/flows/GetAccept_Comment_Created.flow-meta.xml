    <decisions>
        <description>Checks if Comment already has an existing Account and if the related GetAccept Document has one. If Comment does not have one, but Document does, it sets it on Comment.</description>
        <name>Should_we_set_Account_on_Comment</name>
        <label>Should we set Account on Comment?</label>
        <locationX>204</locationX>
        <locationY>323</locationY>
        <defaultConnector>
            <targetReference>Should_we_set_Journal_on_Comment</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Account Set</defaultConnectorLabel>
        <rules>
            <name>Set_Account_on_Comment</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Account__r.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.GetAccept__GetAccept_Document__r.GetAccept__Account__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Account_to_Comment</targetReference>
            </connector>
            <label>Set Account on Comment</label>
        </rules>
    </decisions>
    <decisions>
        <description>Checks if Comment already has an existing Journal and if the related GetAccept Document has one. If Comment does not have one, but Document does, it sets it on Comment.</description>
        <name>Should_we_set_Journal_on_Comment</name>
        <label>Should we set Journal on Comment?</label>
        <locationX>204</locationX>
