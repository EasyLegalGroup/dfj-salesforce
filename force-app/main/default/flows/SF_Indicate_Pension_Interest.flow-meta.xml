    <formulas>
        <name>urlAccountPhone</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!Account.Phone}, 1) = &quot;+&quot;,
   &quot;%2B&quot; &amp; MID({!Account.Phone_Formatted__c}, 2, LEN({!Account.Phone_Formatted__c}) - 1),
   {!Account.Phone_Formatted__c}
)</expression>
    </formulas>
    <formulas>
        <name>urlLeadPhone</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!Get_Lead.Phone_Formatted_No_Spaces__c}, 1) = &quot;+&quot;,
   &quot;%2B&quot; &amp; MID({!Get_Lead.Phone_Formatted_No_Spaces__c}, 2, LEN({!Get_Lead.Phone_Formatted_No_Spaces__c}) - 1),
   {!Get_Lead.Phone_Formatted_No_Spaces__c}
)</expression>
    </formulas>
    <formulas>
        <name>urlUserFirstName</name>
        <dataType>String</dataType>
