    <formulas>
        <name>frmMonth</name>
        <dataType>Number</dataType>
        <expression>MONTH({!$Flow.CurrentDate})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>frmNumTickets</name>
        <dataType>Number</dataType>
        <expression>VALUE({!plNumberOfTickets})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>frmTargetName</name>
        <dataType>String</dataType>
        <expression>UPPER(
  LEFT({!$User.Email}, FIND(&quot;@&quot;, {!$User.Email}) - 1)
)
&amp; &quot; - &quot;
&amp; CASE(
    {!frmMonth},
      1, &quot;January&quot;,   2, &quot;February&quot;,  3, &quot;March&quot;,
      4, &quot;April&quot;,     5, &quot;May&quot;,       6, &quot;June&quot;,
      7, &quot;July&quot;,      8, &quot;August&quot;,    9, &quot;September&quot;,
     10, &quot;October&quot;,  11, &quot;November&quot;, 12, &quot;December&quot;,
    &quot;Invalid Month&quot;
  )
&amp; &quot; &quot;
&amp; TEXT(
    FLOOR({!frmYear})
)</expression>
    </formulas>
    <formulas>
        <name>frmTrimmedName</name>
        <dataType>String</dataType>
        <expression>IF(
  LEN({!Get_Lead.Name}) &gt; 52,
  LEFT({!Get_Lead.Name}, 52) &amp; &quot;...&quot;,
  {!Get_Lead.Name}
)</expression>
    </formulas>
    <formulas>
        <name>frmYear</name>
        <dataType>Number</dataType>
        <expression>YEAR({!$Flow.CurrentDate})</expression>
        <scale>0</scale>
    </formulas>
