    <collectionProcessors>
        <name>Filter_for_Public_Registration_Documents</name>
        <elementSubtype>FilterCollectionProcessor</elementSubtype>
        <label>Filter for Public Registration Documents</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNextValueToReference>currentItem_Filter_for_Public_Registration_Documents</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>Get_Shared_Documents</collectionReference>
        <conditionLogic>and</conditionLogic>
        <conditions>
            <leftValueReference>currentItem_Filter_for_Public_Registration_Documents.Is_Public_Registration_Required__c</leftValueReference>
            <operator>EqualTo</operator>
            <rightValue>
                <booleanValue>true</booleanValue>
            </rightValue>
        </conditions>
        <conditions>
            <leftValueReference>currentItem_Filter_for_Public_Registration_Documents.Public_Registration_Status__c</leftValueReference>
            <operator>EqualTo</operator>
            <rightValue>
                <stringValue>Ready for Public Registration</stringValue>
            </rightValue>
        </conditions>
        <conditions>
            <leftValueReference>currentItem_Filter_for_Public_Registration_Documents.Status__c</leftValueReference>
            <operator>EqualTo</operator>
            <rightValue>
                <stringValue>Approved</stringValue>
            </rightValue>
        </conditions>
        <connector>
            <targetReference>Did_We_Find_Shared_Document_Ready_for_Registration</targetReference>
        </connector>
    </collectionProcessors>
    <collectionProcessors>
