    <decisions>
        <name>Is_Document_Completed</name>
        <label>Is Document Completed?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Assign_IsUnexpected_True</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Document Incomplete</defaultConnectorLabel>
        <rules>
            <name>Document_is_Completed</name>
            <conditionLogic>1 AND (2 OR (3 AND 4))</conditionLogic>
            <conditions>
                <leftValueReference>Loop_Documents.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Approved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Documents.Is_Public_Registration_Required__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Documents.Is_Public_Registration_Required__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Documents.Public_Registration_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Public Registration Complete</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_Documents</targetReference>
            </connector>
            <label>Document is Completed</label>
        </rules>
    </decisions>
