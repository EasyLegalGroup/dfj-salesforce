    <formulas>
        <name>frmWorkMonth</name>
        <dataType>String</dataType>
        <expression>CASE(
    MONTH({!$Record.Date__c}), 
    1, &quot;January&quot;, 
    2, &quot;February&quot;, 
    3, &quot;March&quot;, 
    4, &quot;April&quot;, 
    5, &quot;May&quot;, 
    6, &quot;June&quot;, 
    7, &quot;July&quot;, 
    8, &quot;August&quot;, 
    9, &quot;September&quot;, 
    10, &quot;October&quot;, 
    11, &quot;November&quot;, 
    12, &quot;December&quot;, 
    &quot;&quot;
) 
&amp; &quot; &quot; &amp; TEXT(YEAR({!$Record.Date__c}))</expression>
    </formulas>
