    <description>Runs daily. Checks if the date is 30, 14, 7 or 1 day before the start of an event and then dynamically finds the Email Template that fits that day and sends it to the Host.</description>
    <environments>Default</environments>
    <formulas>
        <name>frmTodayPlus1</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.StartDate} = TODAY() + 1</expression>
    </formulas>
    <formulas>
        <name>frmTodayPlus14</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.StartDate} = TODAY() + 14</expression>
    </formulas>
    <formulas>
        <name>frmTodayPlus30</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.StartDate} = TODAY() + 30</expression>
    </formulas>
    <formulas>
        <name>frmTodayPlus4</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.StartDate} = TODAY() + 4</expression>
    </formulas>
    <formulas>
        <name>frmTodayPlus7</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.StartDate} = TODAY() + 7</expression>
    </formulas>
