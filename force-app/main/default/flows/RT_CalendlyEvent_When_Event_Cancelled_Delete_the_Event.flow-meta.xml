<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>54.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Populate_CustomError_with_Flow_Fault_Message</name>
        <label>Populate CustomError with Flow Fault Message</label>
        <locationX>754</locationX>
        <locationY>492</locationY>
        <assignmentItems>
            <assignToReference>CustomError</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Flow.FaultMessage</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>AL_Send_Error_Email_from_Input_Values</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Did_We_Find_an_Event</name>
        <label>Did We Find an Event?</label>
        <locationX>314</locationX>
        <locationY>492</locationY>
        <defaultConnectorLabel>Event Not Found</defaultConnectorLabel>
        <rules>
            <name>Event_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Event</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Delete_Event</targetReference>
            </connector>
            <label>Event Exists</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_invitee_canceled</name>
        <label>Invitee canceled?</label>
        <locationX>666</locationX>
        <locationY>276</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Invitee_canceled</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>invitee.canceled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Event</targetReference>
            </connector>
            <label>Invitee canceled</label>
        </rules>
    </decisions>
    <description>Is triggered by an incoming CalendlyAction (happens in installed Calendly Package). If it is &quot;invitee.canceled&quot;, then flow tries to find the Event using Calendly Invitee UUID and deletes it, if it finds it.</description>
    <environments>Default</environments>
    <interviewLabel>RT - CalendlyEvent: When Event Cancelled, Delete the Event {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RT - CalendlyEvent: When Event Cancelled, Delete the Event</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordDeletes>
        <description>Deletes the Event found above</description>
        <name>Delete_Event</name>
        <label>Delete Event</label>
        <locationX>50</locationX>
        <locationY>600</locationY>
        <faultConnector>
            <isGoTo>true</isGoTo>
            <targetReference>Populate_CustomError_with_Flow_Fault_Message</targetReference>
        </faultConnector>
        <inputReference>Get_Event</inputReference>
    </recordDeletes>
    <recordLookups>
        <description>Find an Event in Salesforce by looking at the Calendly__InviteeUuid__c</description>
        <name>Get_Event</name>
        <label>Get Event</label>
        <locationX>314</locationX>
        <locationY>384</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Did_We_Find_an_Event</targetReference>
        </connector>
        <faultConnector>
            <targetReference>Populate_CustomError_with_Flow_Fault_Message</targetReference>
        </faultConnector>
        <filterLogic>(1 AND 2) OR (3 AND 4)</filterLogic>
        <filters>
            <field>Calendly__InviteeUuid__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Calendly__InviteeUuid__c</elementReference>
            </value>
        </filters>
        <filters>
            <field>Calendly__InviteeUuid__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Subject</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Calendly__InviteeUuid__c</elementReference>
            </value>
        </filters>
        <filters>
            <field>Subject</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Event</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <sourceTemplate>Calendly__CancelEventTemplate</sourceTemplate>
    <start>
        <locationX>100</locationX>
        <locationY>0</locationY>
        <object>Calendly__CalendlyAction__c</object>
        <recordTriggerType>Create</recordTriggerType>
        <scheduledPaths>
            <name>X1_minute_after</name>
            <connector>
                <targetReference>Is_invitee_canceled</targetReference>
            </connector>
            <label>1 minute after</label>
            <offsetNumber>1</offsetNumber>
            <offsetUnit>Minutes</offsetUnit>
            <timeSource>RecordTriggerEvent</timeSource>
        </scheduledPaths>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
    <subflows>
        <name>AL_Send_Error_Email_from_Input_Values</name>
        <label>AL - Send Error Email from Input Values</label>
        <locationX>754</locationX>
        <locationY>600</locationY>
        <flowName>AL_Send_Error_Email_from_Input_Values</flowName>
        <inputAssignments>
            <name>inputCustomError</name>
            <value>
                <elementReference>CustomError</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>inputFlowName</name>
            <value>
                <stringValue>RT - CalendlyEvent: When Event Cancelled, Delete the Event</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>inputRecordId</name>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </inputAssignments>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </subflows>
    <variables>
        <name>CustomError</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <name>EventRecordId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
    </variables>
</Flow>
