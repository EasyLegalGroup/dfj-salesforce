    <environments>Default</environments>
    <formulas>
        <name>FormattedNationalIdentificationNumber</name>
        <dataType>String</dataType>
        <expression>IF(
    CONTAINS({!$Record.National_Identification_Number__c}, &quot;-&quot;),
    {!$Record.National_Identification_Number__c},
    LEFT({!$Record.National_Identification_Number__c}, 6) &amp; &quot;-&quot; &amp; MID({!$Record.National_Identification_Number__c}, 7, 4)
)</expression>
    </formulas>
    <formulas>
        <name>MaskedIdentificationNumber</name>
        <dataType>String</dataType>
        <expression>IF(
    CONTAINS({!$Record.National_Identification_Number__c}, &quot;-&quot;),
    LEFT({!$Record.National_Identification_Number__c}, LEN({!$Record.National_Identification_Number__c}) - 4) &amp; &quot;****&quot;,
    LEFT({!$Record.National_Identification_Number__c}, 6) &amp; &quot;-&quot; &amp; MID({!$Record.National_Identification_Number__c}, 7, LEN({!$Record.National_Identification_Number__c}) - 6 - 4) &amp; &quot;****&quot;
)</expression>
    </formulas>
