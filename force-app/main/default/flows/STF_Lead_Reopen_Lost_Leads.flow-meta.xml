        <label>When Was Lead Closed?</label>
        <locationX>182</locationX>
        <locationY>252</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>More_Than_90_Days_Ago</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>frmIsMoreThan90DaysOld</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Lead</targetReference>
            </connector>
            <label>More Than 90 Days Ago</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>frmIsMoreThan90DaysOld</name>
        <dataType>Boolean</dataType>
        <expression>TODAY() - DATEVALUE({!$Record.Closed_Date__c}) &gt; 90</expression>
    </formulas>
