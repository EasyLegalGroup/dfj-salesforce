    BEGINS(SUBSTITUTE({!frmCustomerToVerify},&quot; &quot;,&quot;&quot;), &quot;+&quot;),
    &quot;SMS&quot;,
    &quot;Invalid&quot;
  )
)</expression>
    </formulas>
