    <formulas>
        <name>frmCustomerToVerify</name>
        <dataType>String</dataType>
        <expression>IF(
  CONTAINS({!customerToVerify},&quot;@&quot;),
  {!customerToVerify},
  IF(
    BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+45&quot;),
    SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
    IF(
      BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;45&quot;),
      &quot;+&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
      IF(
        AND(
          NOT(BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+45&quot;)),
          OR(
            {!Country}=&quot;Denmark&quot;,
            {!Country}=&quot;Testamente_DK&quot;
          )
        ),
        &quot;+45&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
        IF(
          AND(
            NOT(BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+&quot;)),
            OR(
              {!Country}=&quot;Denmark&quot;,
              {!Country}=&quot;Testamente_DK&quot;
            )
          ),
          &quot;+&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
          IF(
            BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+46&quot;),
            SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
            IF(
              BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;46&quot;),
              &quot;+&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
              IF(
                AND(
                  NOT(BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+46&quot;)),
                  {!Country}=&quot;Sweden&quot;
                ),
                &quot;+46&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
                IF(
                  AND(
                    NOT(BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+&quot;)),
                    {!Country}=&quot;Sweden&quot;
                  ),
                  &quot;+&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
                  IF(
                    BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+353&quot;),
                    SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
                    IF(
                      BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;353&quot;),
                      &quot;+&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
                      IF(
                        AND(
                          NOT(BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+353&quot;)),
                          {!Country}=&quot;Ireland&quot;
                        ),
                        &quot;+353&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
                        IF(
                          AND(
                            NOT(BEGINS(SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),&quot;+&quot;)),
                            {!Country}=&quot;Ireland&quot;
                          ),
                          &quot;+&quot; &amp; SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;),
                          SUBSTITUTE({!customerToVerify},&quot; &quot;,&quot;&quot;)
                        )
                      )
                    )
                  )
                )
              )
            )
          )
        )
      )
    )
  )
)</expression>
    </formulas>
    <formulas>
        <name>frmMedium</name>
        <dataType>String</dataType>
        <expression>IF(
  CONTAINS({!frmCustomerToVerify},&quot;@&quot;),
  &quot;email&quot;,
