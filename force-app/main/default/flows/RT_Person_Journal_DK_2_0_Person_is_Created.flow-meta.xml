    <formulas>
        <name>frmBirthday</name>
        <dataType>Date</dataType>
        <expression>DATE(
    VALUE({!$Record.Year_of_Birth__c}),
    VALUE({!$Record.Month_of_Birth__c}),
    VALUE({!$Record.Day_of_Birth__c})
)</expression>
    </formulas>
    <formulas>
        <name>frmFirstName</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!$Record.person_fornavn__c}),
  &quot;UNKNOWN FIRST NAME&quot;,
  IF(
    LEN({!$Record.person_fornavn__c}) &lt;= 40,
    {!$Record.person_fornavn__c},
    LEFT({!$Record.person_fornavn__c}, 37) &amp; &quot;...&quot;
  )
)</expression>
    </formulas>
    <formulas>
        <name>frmIsEmailValid</name>
        <dataType>Boolean</dataType>
        <expression>REGEX({!$Record.person_email__c}, &quot;^[a-zA-Z0-9._%+-]+@[a-zA-Z0-9-]+(\\.[a-zA-Z0-9-]+)*\\.[a-zA-Z]{2,4}$&quot;)</expression>
    </formulas>
    <formulas>
        <name>frmLastName</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!$Record.person_efternavn__c}), &apos;UNKNOWN LAST NAME&apos;, {!$Record.person_efternavn__c})</expression>
    </formulas>
    <formulas>
        <name>frmParent</name>
        <dataType>String</dataType>
        <expression>IF(
    {!$Record.person_hidden_barn_flles__c},
    &quot;Child of Both Parents&quot;,
    IF(
        {!$Record.person_hidden_barn_p1__c},
        &quot;Child of Person 1&quot;,
        IF(
            {!$Record.person_hidden_barn_p2__c},
            &quot;Child of Person 2&quot;,
            IF(
                {!$Record.person_hidden_barn_single__c},
                &quot;Child of Single Parent&quot;,
                &quot;&quot;
            )
        )
    )
)</expression>
    </formulas>
    <formulas>
        <name>frmPersonType</name>
        <dataType>String</dataType>
        <expression>IF(
    {!$Record.person_hidden_person_1__c},
    &quot;Primary Person&quot;,
    IF(
        {!$Record.person_hidden_person_2__c},
        &quot;Spouse / Partner&quot;,
        IF(
            {!$Record.person_hf_barn__c},
            &quot;Child&quot;,
            &quot;Unspecified&quot;
        )
    )
)</expression>
    </formulas>
