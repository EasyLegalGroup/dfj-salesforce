<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Assign_Account_s_Market_Unit_to_Variable</name>
        <label>Assign Account&apos;s Market Unit to Variable</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>MarketUnit</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Account__r.Market_Unit__c</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Shared_Document</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Assign_Journal_s_Market_Unit_to_Variable</name>
        <label>Assign Journal&apos;s Market Unit to Variable</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>MarketUnit</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Journal__r.Market_Unit__c</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Shared_Document</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>What_Type_of_Related_Record_ID</name>
        <label>What Type of Related Record ID?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Related_Journal_ID</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Related_Record_ID__c</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>a0A</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Journal</targetReference>
            </connector>
            <label>Related Journal ID</label>
        </rules>
        <rules>
            <name>Related_Account_ID</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Related_Record_ID__c</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>001</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Account</targetReference>
            </connector>
            <label>Related Account ID</label>
        </rules>
    </decisions>
    <decisions>
        <name>Which_Relation_Do_We_Have</name>
        <label>Which Relation Do We Have?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Got_Journal</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Journal__r.Market_Unit__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Journal_s_Market_Unit_to_Variable</targetReference>
            </connector>
            <label>Got Journal</label>
        </rules>
        <rules>
            <name>Got_Account</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Account__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Account_s_Market_Unit_to_Variable</targetReference>
            </connector>
            <label>Got Account</label>
        </rules>
        <rules>
            <name>Got_Related_Record_ID</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Related_Record_ID__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>What_Type_of_Related_Record_ID</targetReference>
            </connector>
            <label>Got Related Record ID</label>
        </rules>
    </decisions>
    <description>If a Shared Document is created without a Market Unit, this flow checks related records to see if we can derive the Market Unit from one of these.</description>
    <environments>Default</environments>
    <interviewLabel>RT - Shared Document: Set Market Unit when Created {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RT - Shared Document: Set Market Unit when Created</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <description>Sets Account&apos;s Market Unit in the MarketUnit Variable</description>
        <name>Get_Account</name>
        <label>Get Account</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Update_Shared_Document</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Related_Record_ID__c</elementReference>
            </value>
        </filters>
        <object>Account</object>
        <outputAssignments>
            <assignToReference>MarketUnit</assignToReference>
            <field>Market_Unit__c</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <description>Sets Journal&apos;s Market Unit in the MarketUnit Variable</description>
        <name>Get_Journal</name>
        <label>Get Journal</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Update_Shared_Document</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Related_Record_ID__c</elementReference>
            </value>
        </filters>
        <object>Journal__c</object>
        <outputAssignments>
            <assignToReference>MarketUnit</assignToReference>
            <field>Market_Unit__c</field>
        </outputAssignments>
    </recordLookups>
    <recordUpdates>
        <description>Sets MarketUnit on the triggering Shared Document</description>
        <name>Update_Shared_Document</name>
        <label>Update Shared Document</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <inputAssignments>
            <field>Market_Unit__c</field>
            <value>
                <elementReference>MarketUnit</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Which_Relation_Do_We_Have</targetReference>
        </connector>
        <filterLogic>(1 OR 2 OR 3) AND 4</filterLogic>
        <filters>
            <field>Account__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Journal__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Related_Record_ID__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Market_Unit__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Shared_Document__c</object>
        <recordTriggerType>Create</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
    <variables>
        <name>MarketUnit</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
    </variables>
</Flow>
