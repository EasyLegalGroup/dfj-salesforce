        </rules>
        <rules>
            <name>Tinglysning</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Journal_States__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Tinglysning</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Journal_States__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Intern - Tinglysning haster</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Tinglysning_Values_and_Clear_Fields</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>Tinglysning</label>
        </rules>
        <rules>
            <name>Complete</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Journal_States__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>complete</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Journal_States__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Færdig ny process</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Does_Journal_Have_an_Account</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>Complete</label>
        </rules>
    </decisions>
