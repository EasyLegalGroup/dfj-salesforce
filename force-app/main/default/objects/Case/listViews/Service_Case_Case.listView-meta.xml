<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Service_Case_Case</fullName>
    <booleanFilter>(1 OR 2) AND 3 AND 4 AND 5 AND 6</booleanFilter>
    <columns>CASES.NAME</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>CASES.CREATED_DATE</columns>
    <columns>CASES.STATUS</columns>
    <columns>Case_Duration_Since_Open_Reopen_Days__c</columns>
    <columns>Case_Duration_Days__c</columns>
    <columns>NAME</columns>
    <columns>CASES.ORIGIN</columns>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>OWNER_NAME</columns>
