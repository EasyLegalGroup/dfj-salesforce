<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OrderPriceNocancellation__c</fullName>
    <description>Order Price - Order with status &quot;Canceled&quot;</description>
    <formula>IF(ISPICKVAL( Status , &quot;Canceled&quot;),0, Order_Price__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Order Price - No cancellation</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
