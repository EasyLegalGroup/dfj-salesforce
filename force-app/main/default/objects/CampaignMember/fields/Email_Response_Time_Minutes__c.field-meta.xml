<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Email_Response_Time_Minutes__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
    NOT(ISBLANK(Time_of_Email_Form_Reply__c)),
    ROUND(MOD((Time_of_Email_Form_Reply__c - Time_of_Email_Form_Invite__c) * 24 * 60, 60), 0),
    NULL
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Email Response Time (Minutes)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
