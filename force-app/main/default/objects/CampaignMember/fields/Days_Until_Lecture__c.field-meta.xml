<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_Until_Lecture__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
    ISBLANK(Campaign.Start__c),
    NULL,
    (DATEVALUE(Campaign.Start__c) - DATEVALUE(NOW()))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Days Until Lecture</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
