<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SMS_Response_Time_Hours__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
    NOT(ISBLANK(Time_of_Lecture_Form_Reply__c)),
    FLOOR(MOD((Time_of_Lecture_Form_Reply__c - Time_of_Lecture_Form_Invite__c) * 24, 24)),
    NULL
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>SMS Response Time (Hours)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
