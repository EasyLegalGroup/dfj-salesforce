<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Unconvert_Lead</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISCHANGED(Status),
    PRIORVALUE(Status) = &quot;Converted&quot;
)</errorConditionFormula>
    <errorMessage>You cannot unconvert a Lead. Contact your administrator.</errorMessage>
</ValidationRule>
