<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsCallable__c</fullName>
    <formula>AND(OR(telegenta__Next_Call_Time__c &lt; NOW(), ISNULL(telegenta__Next_Call_Time__c)), OR(ISPICKVAL(Status, &quot;New&quot;),ISPICKVAL(Status, &quot;Working&quot;)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>IsCallableNewWorking</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
