<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Average_Order_Amount_on_No_Show__c</fullName>
    <formula>IF(
    Revenue_on_No_Show_Incl_VAT__c = 0,
    0,
    Revenue_on_No_Show_Incl_VAT__c / Count_of_No_Show__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Revenue on No Show Incl. VAT / Count of No Show</inlineHelpText>
    <label>Average Order Amount on No Show</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
