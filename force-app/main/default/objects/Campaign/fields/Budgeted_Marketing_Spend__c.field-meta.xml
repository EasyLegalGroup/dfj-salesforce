<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Budgeted_Marketing_Spend__c</fullName>
    <formula>CASE(
Market_Unit__c,
&apos;DFJ_DK&apos;, ((Capacity__c * 0.45) * $CustomMetadata.Currency_Multiplier_Configuration__mdt.Din_Familie_Jurist.Cost_Multiplier_Value__c * 0.8),
&apos;FA_SE&apos;, ((Capacity__c * 0.45) * $CustomMetadata.Currency_Multiplier_Configuration__mdt.dinfamiliejurist_nu.Cost_Multiplier_Value__c * 0.8),
&apos;Testamente_DK&apos;, ((Capacity__c * 0.45) * $CustomMetadata.Currency_Multiplier_Configuration__mdt.Testamente_Danmark.Cost_Multiplier_Value__c * 0.8),
&apos;Testamente_SV&apos;, ((Capacity__c * 0.45) * $CustomMetadata.Currency_Multiplier_Configuration__mdt.Testamente_Sverige.Cost_Multiplier_Value__c * 0.8),
0
