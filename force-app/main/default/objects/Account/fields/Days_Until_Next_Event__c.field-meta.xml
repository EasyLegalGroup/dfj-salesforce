<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_Until_Next_Event__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  OR(
    ISBLANK( Date_of_Next_Event__c ),
    Date_of_Next_Event__c &lt; TODAY()
  ),
  NULL,
  Date_of_Next_Event__c - TODAY()
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Days Until Next Event</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
